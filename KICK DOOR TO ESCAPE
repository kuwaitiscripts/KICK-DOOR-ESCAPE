-- 6. God Mode
Tab:AddToggle({
    Name = "ğŸ›¡ï¸ God Mode",
    Default = false,
    Callback = function(Value)
        if Value then
            Player.Character.Humanoid.WalkSpeed = 50
            Player.Character.Humanoid.JumpPower = 100
        else
            Player.Character.Humanoid.WalkSpeed = 16
            Player.Character.Humanoid.JumpPower = 50
        end
    end
})

-- 7. Door ESP
Tab:AddToggle({
    Name = "ğŸ‘ï¸ Door ESP",
    Default = false,
    Callback = function(Value)
        _G.DoorESP = Value
        while _G.DoorESP do
            for _, obj in ipairs(Workspace:GetDescendants()) do
                if obj.Name:find("Door") and obj:IsA("BasePart") and not obj:FindFirstChild("Highlight") then
                    local hl = Instance.new("Highlight", obj)
                    hl.FillColor = Color3.fromRGB(255, 255, 0)
                end
            end
            task.wait(2)
        end
    end
})

-- 8. Auto Coins
Tab:AddToggle({
    Name = "ğŸ’° Auto Coins",
    Default = false,
    Callback = function(Value)
        _G.AutoCoins = Value
        while _G.AutoCoins do
            for _, obj in ipairs(Workspace:GetDescendants()) do
                if obj.Name:find("Coin") and obj:IsA("BasePart") then
                    local p = obj:FindFirstChildWhichIsA("ProximityPrompt")
                    if p then fireproximityprompt(p) end
                end
            end
            task.wait(1)
        end
    end
})

-- 9. Skip Cutscenes
Tab:AddToggle({
    Name = "ğŸ¬ Skip Cutscenes",
    Default = false,
    Callback = function(Value)
        _G.SkipCuts = Value
        while _G.SkipCuts do
            for _, obj in ipairs(Workspace:GetChildren()) do
                if obj.Name:find("Cutscene") then pcall(function() obj:Destroy() end) end
            end
            task.wait(1)
        end
    end
})

-- 10. Bypass Dummy Traffic
Tab:AddToggle({
    Name = "ğŸ­ Bypass Traffic",
    Default = false,
    Callback = function(Value)
        _G.Traffic = Value
        while _G.Traffic do
            pcall(function() RemoteEvents.Click:FireServer() end)
            task.wait(2)
        end
    end
})

