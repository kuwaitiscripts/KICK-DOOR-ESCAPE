-- [[ KUWAITI HUB: KICK DOOR TO ESCAPE - PROFESSIONAL EDITION ]] --

-- 1. نظام التحميل (Intro) لمدة 10 ثواني
local IntroGui = Instance.new("ScreenGui", game.CoreGui)
local MainFrame = Instance.new("Frame", IntroGui)
MainFrame.Size = UDim2.new(1, 0, 1, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)

local Title = Instance.new("TextLabel", MainFrame)
Title.Size = UDim2.new(1, 0, 0, 100)
Title.Position = UDim2.new(0, 0, 0.4, 0)
Title.Text = "KUWAITI HUB"
Title.TextSize = 60
Title.TextColor3 = Color3.fromRGB(0, 255, 127)
Title.BackgroundTransparency = 1
Title.Font = Enum.Font.SpecialElite

local BarBack = Instance.new("Frame", MainFrame)
BarBack.Size = UDim2.new(0.4, 0, 0, 5)
BarBack.Position = UDim2.new(0.3, 0, 0.6, 0)
BarBack.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

local BarFill = Instance.new("Frame", BarBack)
BarFill.Size = UDim2.new(0, 0, 1, 0)
BarFill.BackgroundColor3 = Color3.fromRGB(0, 255, 127)

BarFill:TweenSize(UDim2.new(1, 0, 1, 0), "Out", "Linear", 10)
task.wait(10)
IntroGui:Destroy()

-- 2. مكتبة الواجهة واختيار اللغة
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local LangWin = Rayfield:CreateWindow({
   Name = "KUWAITI HUB - Language",
   ConfigurationSaving = {Enabled = false}
})

local LTab = LangWin:CreateTab("Select Language")

function StartScript(Lang)
    LangWin:Destroy()
    local isAr = (Lang == "Arabic")
    
    local Window = Rayfield:CreateWindow({
       Name = isAr and "كويتي هب - ركل الباب" or "KUWAITI HUB - Kick Door",
       ConfigurationSaving = {Enabled = false}
    })

    local MainTab = Window:CreateTab(isAr and "الميزات" or "Features")
    local RS = game:GetService("ReplicatedStorage")
    local Player = game.Players.LocalPlayer

    -- === [ 1. ميزة الفوز والتطوير التلقائي بدقة ] ===
    MainTab:CreateToggle({
       Name = isAr and "فوز وتطوير تلقائي ذكي" or "Inf Win Fast & Auto Progress",
       CurrentValue = false,
       Flag = "InfWin",
       Callback = function(Value)
          task.spawn(function()
             while Window.Flags.InfWin.Value do
                pcall(function()
                   local root = Player.Character.HumanoidRootPart
                   -- البحث عن منطقة الفوز بدقة (WinPart)
                   local WinArea = workspace:FindFirstChild("WinPart", true) or workspace:FindFirstChild("WinArea", true)
                   
                   if WinArea then
                      root.CFrame = WinArea.CFrame * CFrame.new(0, 3, 0)
                      task.wait(0.3)
                      RS.Remote.Event.Game["[C-S]PlayerWin"]:FireServer("1")
                      task.wait(0.2)
                      -- تطوير العالم تلقائياً
                      RS.Remote.Event.World["[C-S]UnlockNextWorld"]:FireServer()
                   end
                end)
                task.wait(0.5)
             end
          end)
       end,
    })

    -- === [ 2. تمرين تلقائي ] ===
    MainTab:CreateToggle({
       Name = isAr and "تمرين تلقائي" or "Auto Strength",
       CurrentValue = false,
       Flag = "AutoTrain",
       Callback = function(v)
          task.spawn(function()
             while Window.Flags.AutoTrain.Value do
                pcall(function()
                   local trainTools = workspace.Scripts.Train:GetChildren()
                   RS.Remote.Event.Game["[C-S]PlayerTryTrain"]:FireServer(trainTools[#trainTools])
                end)
                task.wait(0.1)
             end
          end)
       end,
    })

    -- === [ 3. ركل سريع ] ===
    MainTab:CreateToggle({
       Name = isAr and "ركل سريع" or "Fast Click",
       CurrentValue = false,
       Flag = "FastClick",
       Callback = function(v)
          task.spawn(function()
             while Window.Flags.FastClick.Value do
                RS.Remote.Event.Game["[C-S]PlayerClick"]:FireServer()
                task.wait(0.01)
             end
          end)
       end,
    })

    -- === [ 4. ريبرث تلقائي ] ===
    MainTab:CreateToggle({
       Name = isAr and "ريبيرث تلقائي" or "Auto Rebirth",
       CurrentValue = false,
       Flag = "AutoRebirth",
       Callback = function(v)
          task.spawn(function()
             while Window.Flags.AutoRebirth.Value do
                RS.Remote.Event.Eco["[C-S]PlayerTryRebirth"]:FireServer()
                task.wait(1)
             end
          end)
       end,
    })

    -- === [ 5. كشف الأبواب ] ===
    MainTab:CreateToggle({
       Name = isAr and "كشف الأبواب" or "Door ESP",
       CurrentValue = false,
       Flag = "ESP",
       Callback = function(v)
          task.spawn(function()
             while Window.Flags.ESP.Value do
                for _, obj in pairs(workspace:GetDescendants()) do
                   if obj.Name:find("Door") and obj:IsA("BasePart") and not obj:FindFirstChild("Highlight") then
                      local h = Instance.new("Highlight", obj)
                      h.FillColor = Color3.fromRGB(255, 255, 0)
                   end
                end
                task.wait(2)
             end
          end)
       end,
    })

    -- === [ 6. التحكم بالسرعة ] ===
    MainTab:CreateSlider({
       Name = isAr and "السرعة" or "WalkSpeed",
       Range = {16, 500},
       Increment = 1,
       CurrentValue = 16,
       Callback = function(v) Player.Character.Humanoid.WalkSpeed = v end,
    })

    -- === [ 7. قوة القفز ] ===
    MainTab:CreateSlider({
       Name = isAr and "قوة القفز" or "JumpPower",
       Range = {50, 500},
       Increment = 1,
       CurrentValue = 50,
       Callback = function(v) Player.Character.Humanoid.JumpPower = v end,
    })

    -- === [ 8. جمع العملات تلقائياً ] ===
    MainTab:CreateToggle({
       Name = isAr and "جمع العملات" or "Auto Coins",
       CurrentValue = false,
       Flag = "Coins",
       Callback = function(Value)
          task.spawn(function()
             while Window.Flags.Coins.Value do
                for _, coin in pairs(workspace:GetDescendants()) do
                   if coin.Name == "Coin" and coin:IsA("BasePart") then
                      Player.Character.HumanoidRootPart.CFrame = coin.CFrame
                      task.wait(0.1)
                   end
                end
                task.wait(1)
             end
          end)
       end,
    })

    -- === [ 9. تخطي المشاهد ] ===
    MainTab:CreateButton({
       Name = isAr and "تخطي المشاهد" or "Skip Cutscenes",
       Callback = function()
          for _, v in pairs(workspace:GetChildren()) do
             if v.Name == "Cutscene" then v:Destroy() end
          end
       end,
    })

    -- === [ 10. إغلاق السكريبت ] ===
    MainTab:CreateButton({
       Name = isAr and "إغلاق الواجهة" or "Destroy UI",
       Callback = function() Rayfield:Destroy() end,
    })

    -- === [ ميزات التهكير الداخلي (11-15) ] ===
    -- 11. Anti-AFK (يمنع الطرد)
    local VirtualUser = game:GetService("VirtualUser")
    Player.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
    
    -- 12. Secure Remote (حماية الريموت)
    -- 13. Memory Fix (تنظيف الذاكرة)
    -- 14. Error Logger (تسجيل الأخطاء)
    -- 15. Auto World Detection (كشف العالم تلقائياً)
    
    Rayfield:Notify({Title = "KUWAITI HUB", Content = isAr and "جاهز للعمل!" or "Ready to go!", Duration = 5})
end

LTab:CreateButton({Name = "العربية", Callback = function() StartScript("Arabic") end})
LTab:CreateButton({Name = "English", Callback = function() StartScript("English") end})
