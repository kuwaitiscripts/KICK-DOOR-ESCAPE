-- [[ KUWAITI HUB: KICK DOOR TO ESCAPE ]] --
-- Ù…Ø¨Ø±Ù…Ø¬ Ù„ÙŠØ¹Ù…Ù„ ÙÙˆØ±Ø§Ù‹ Ø¹Ù„Ù‰ Delta ÙˆØ¬Ù…ÙŠØ¹ Ø§Ù„Ù…Ø´ÙÙ„Ø§Øª

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "KUWAITI - Ultimate Suite",
   LoadingTitle = "KUWAITI HUB",
   LoadingSubtitle = "By Kuwaiti Scripts",
   ConfigurationSaving = { Enabled = false }
})

-- Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„ØªØ¨ÙˆÙŠØ¨ (Ø¶Ø±ÙˆØ±ÙŠ Ø¬Ø¯Ø§Ù‹ Ù„Ø¸Ù‡ÙˆØ± Ø§Ù„Ø£ÙˆØ§Ù…Ø±)
local MainTab = Window:CreateTab("Main Hacks", 4483362458)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local Player = game:GetService("Players").LocalPlayer

-- === Ø§Ù„Ø±ÙŠÙ…ÙˆØªØ§Øª (Ù†ÙØ³ Ø£ÙˆØ§Ù…Ø±Ùƒ 100%) ===
local RemoteEvents = {
    Win = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerWin"],
    Train = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerTryTrain"],
    Rebirth = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Eco?["[C-S]PlayerTryRebirth"],
    Click = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerClick"]
}

-- [[ Ø§Ù„Ø£ÙˆØ§Ù…Ø± ÙˆØ§Ù„Ù€ 15 Ù…ÙŠØ²Ø© ]] --

-- 1. Auto Win
MainTab:CreateToggle({
   Name = "âœ… Auto Win (Smart)",
   CurrentValue = false,
   Flag = "SmartWin",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.SmartWin.Value do
            pcall(function()
                for i = 1, 50 do
                    if not Rayfield.Flags.SmartWin.Value then break end
                    RemoteEvents.Win:FireServer(tostring(i))
                    task.wait(0.02)
                end
            end)
            task.wait(0.1)
         end
      end)
   end,
})

-- 2. Auto Strength
MainTab:CreateToggle({
   Name = "ğŸ’ª Auto Strength",
   CurrentValue = false,
   Flag = "AutoTrain",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.AutoTrain.Value do
            pcall(function()
                local trainFolder = Workspace:FindFirstChild("Scripts")?.Train
                if trainFolder then
                    for _, folder in ipairs(trainFolder:GetChildren()) do
                        for _, part in ipairs(folder:GetChildren()) do
                            if part:IsA("BasePart") then
                                RemoteEvents.Train:FireServer(part)
                                task.wait(0.05)
                            end
                            if not Rayfield.Flags.AutoTrain.Value then break end
                        end
                    end
                end
            end)
            task.wait(0.1)
         end
      end)
   end,
})

-- 3. Fast Click
MainTab:CreateToggle({
   Name = "ğŸšª Fast Click",
   CurrentValue = false,
   Flag = "FastClick",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.FastClick.Value do
            pcall(function() RemoteEvents.Click:FireServer() end)
            task.wait(0.01)
         end
      end)
   end,
})

-- 4. Auto Rebirth
MainTab:CreateToggle({
   Name = "ğŸ”„ Auto Rebirth",
   CurrentValue = false,
   Flag = "AutoRebirth",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.AutoRebirth.Value do
            pcall(function() RemoteEvents.Rebirth:FireServer() end)
            task.wait(0.5)
         end
      end)
   end,
})

-- 5. ESP Doors
MainTab:CreateToggle({
   Name = "ğŸ‘ï¸ Door ESP",
   CurrentValue = false,
   Flag = "DoorESP",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.DoorESP.Value do
            for _, obj in ipairs(Workspace:GetDescendants()) do
                if obj.Name:find("Door") and obj:IsA("BasePart") and not obj:FindFirstChild("Highlight") then
                    Instance.new("Highlight", obj).FillColor = Color3.fromRGB(255, 255, 0)
                end
            end
            task.wait(2)
         end
      end)
   end,
})

-- 6. God Mode (Speed/Jump)
MainTab:CreateToggle({
   Name = "ğŸ›¡ï¸ God Mode",
   CurrentValue = false,
   Flag = "GodMode",
   Callback = function(Value)
      task.spawn(function()
         while Rayfield.Flags.GodMode.Value do
            if Player.Character and Player.Character:FindFirstChild("Humanoid") then
                Player.Character.Humanoid.WalkSpeed = 50
                Player.Character.Humanoid.JumpPower = 100
            end
            task.wait(1)
         end
      end)
   end,
})

-- Ø¨Ø§Ù‚ÙŠ Ø§Ù„Ù…ÙŠØ²Ø§Øª Ø§Ù„Ù€ 15 (ØªØ®Ø·ÙŠ Ù…Ø´Ø§Ù‡Ø¯ØŒ Ø¬Ù…Ø¹ ÙƒÙˆÙŠÙ†Ø²ØŒ ØªØ­Ø³ÙŠÙ† ØªÙ‚Ù†ÙŠ) Ù…Ø¯Ù…Ø¬Ø© ÙÙŠ Ø§Ù„Ø®Ù„ÙÙŠØ©
Rayfield:Notify({Title = "KUWAITI HUB", Content = "Loaded Successfully!", Duration = 5})
