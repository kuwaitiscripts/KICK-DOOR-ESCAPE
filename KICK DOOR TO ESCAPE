    Callback = function(Value)
        _G.FastClick = Value
        while _G.FastClick do
            pcall(function() RemoteEvents.Click:FireServer() end)
            task.wait(0.01)
        end
    end
})

-- 5. Auto Rebirth
Tab:AddToggle({
    Name = "ğŸ”„ Auto Rebirth",
    Default = false,
    Callback = function(Value)
        _G.AutoRebirth = Value
        while _G.AutoRebirth do
            pcall(function() RemoteEvents.Rebirth:FireServer() end)
