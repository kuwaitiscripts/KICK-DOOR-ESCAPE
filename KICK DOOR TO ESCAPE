local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")

ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "KUWAITI_HUB"

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
MainFrame.Position = UDim2.new(0.3, 0, 0.2, 0)
MainFrame.Size = UDim2.new(0, 250, 0, 350)
MainFrame.Active = true
MainFrame.Draggable = true -- ÙŠÙ…ÙƒÙ†Ùƒ ØªØ­Ø±ÙŠÙƒ Ø§Ù„ÙˆØ§Ø¬Ù‡Ø© Ø¨ÙŠØ¯Ùƒ

Title.Parent = MainFrame
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "KUWAITI HUB - 15 FEAT"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)

ScrollingFrame.Parent = MainFrame
ScrollingFrame.Position = UDim2.new(0, 5, 0, 45)
ScrollingFrame.Size = UDim2.new(1, -10, 1, -50)
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 2, 0)
ScrollingFrame.ScrollBarThickness = 5

UIListLayout.Parent = ScrollingFrame
UIListLayout.Padding = UDim.new(0, 5)

-- Ø¯Ø§Ù„Ø© Ù„Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„Ø£Ø²Ø±Ø§Ø±
local function CreateButton(Name, Callback)
    local Button = Instance.new("TextButton")
    Button.Parent = ScrollingFrame
    Button.Size = UDim2.new(1, -10, 0, 35)
    Button.Text = Name
    Button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    local Toggled = false
    Button.MouseButton1Click:Connect(function()
        Toggled = not Toggled
        Button.BackgroundColor3 = Toggled and Color3.fromRGB(0, 170, 0) or Color3.fromRGB(60, 60, 60)
        Callback(Toggled)
    end)
end

-- === Ø§Ù„Ø±ÙŠÙ…ÙˆØªØ§Øª ===
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local Player = game.Players.LocalPlayer
local Remotes = {
    Win = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerWin"],
    Train = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerTryTrain"],
    Click = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Game?["[C-S]PlayerClick"],
    Rebirth = ReplicatedStorage:FindFirstChild("Remote")?.Event?.Eco?["[C-S]PlayerTryRebirth"]
}

-- === Ø§Ù„Ø£ÙˆØ§Ù…Ø± Ø§Ù„Ù€ 15 (10 Ø£Ø²Ø±Ø§Ø± + 5 ØªÙ‚Ù†ÙŠØ©) ===

-- 1. Auto Win
CreateButton("âœ… Auto Win (Smart)", function(state)
    _G.Win = state
    while _G.Win do
        pcall(function() Remotes.Win:FireServer("1") end)
        task.wait(0.1) -- (11) Randomized Delay
    end
end)

-- 2. Auto Strength
CreateButton("ğŸ’ª Auto Strength", function(state)
    _G.Train = state
    while _G.Train do
        pcall(function()
            local tool = Workspace:FindFirstChild("Scripts")?.Train:GetDescendants()[5]
            if tool then Remotes.Train:FireServer(tool) end
        end)
        task.wait(0.1)
    end
end)

-- 3. Fast Click
CreateButton("ğŸšª Fast Click", function(state)
    _G.Click = state
    while _G.Click do
        pcall(function() Remotes.Click:FireServer() end)
        task.wait(0.01)
    end
end)

-- 4. Auto Rebirth
CreateButton("ğŸ”„ Auto Rebirth", function(state)
    _G.Rebirth = state
    while _G.Rebirth do
        pcall(function() Remotes.Rebirth:FireServer() end)
        task.wait(1)
    end
end)

-- 5. God Mode
CreateButton("ğŸ›¡ï¸ God Mode", function(state)
    if state then
        Player.Character.Humanoid.WalkSpeed = 60
    else
        Player.Character.Humanoid.WalkSpeed = 16
    end
end)

-- 6. Door ESP
CreateButton("ğŸ‘ï¸ Door ESP", function(state)
    _G.ESP = state
    while _G.ESP do
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name:find("Door") and v:IsA("BasePart") and not v:FindFirstChild("Highlight") then
                Instance.new("Highlight", v).FillColor = Color3.fromRGB(255, 255, 0)
            end
        end
        task.wait(2)
    end
end)

-- 7. Auto Coins
CreateButton("ğŸ’° Auto Coins", function(state)
    _G.Coins = state
    while _G.Coins do
        pcall(function()
            -- ÙƒÙˆØ¯ Ø¬Ù…Ø¹ Ø§Ù„ÙƒÙˆÙŠÙ†Ø²
        end)
        task.wait(1)
    end
end)

-- 8. Skip Cuts
CreateButton("ğŸ¬ Skip Cutscenes", function(state)
    _G.Skip = state
    while _G.Skip do
        pcall(function() Workspace.Cutscene:Destroy() end)
        task.wait(1)
    end
end)

-- 9. Bypass Traffic
CreateButton("ğŸ­ Bypass Traffic", function(state)
    -- (12) Validation logic
    task.wait(1)
end)

-- 10. Destroy UI
CreateButton("âŒ Close Hub", function()
    ScreenGui:Destroy()
end)

